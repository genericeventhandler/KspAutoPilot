<Window x:Class="KSPAutoPilot.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KSPAutoPilot"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBox x:Name="OrbitHeight" HorizontalAlignment="Left" Height="23" Margin="36,34,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="ButtonToOrbit" Content="Takeoff" HorizontalAlignment="Left" Margin="174,34,0,0" VerticalAlignment="Top" Width="75" Click="buttonToOrbit_Click"/>
        <Button x:Name="ButtonChangeOrbit" Content="Change Orbit" HorizontalAlignment="Left" Margin="269,33,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
